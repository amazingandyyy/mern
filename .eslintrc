{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true,
      "jest/globals": true
  },
  "parserOptions": {
      "sourceType": "module"
  },
  "extends": [
      "standard",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime"
  ],
  "rules": {
      "react/jsx-filename-extension": 0,
      "react/jsx-uses-vars": 2,
      "react/prop-types": 0,
      "import/prefer-default-export": 0,
      "no-console": 0
  },
  "plugins": [
      "react",
      "node",
      "jest"
  ]
}