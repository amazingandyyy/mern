{
  "name": "mern-client",
  "description": "Simple starter package for Redux with React and Babel support",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "dev": "npm ls react && webpack-dev-server --open --config webpack.dev.js",
    "start": "serve ../docs",
    "build": "rm -rf ../docs && webpack --config webpack.prod.js && npm run copy:static",
    "copy:static": "cp -rf ./src/statics ../docs",
    "dev:parcel": "parcel ./src/index.html --open"
  },
  "author": "amazingandyyy",
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.2",
    "buffer": "^6.0.3",
    "css-loader": "^6.3.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.1",
    "node-sass": "^7.0.3",
    "sass-loader": "^10.1.1",
    "serve": "^12.0.0",
    "style-loader": "^3.0.0",
    "webpack": "^5.68.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.27.2",
    "parcel": "^2.8.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.0",
    "use-form-react": "^0.0.9"
  }
}
