{
  "name": "mern-client",
  "description": "Simple starter package for Redux with React and Babel support",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "dev": "npm ls react && webpack-dev-server --open --config webpack.dev.js",
    "start": "serve ../docs",
    "build": "rm -rf ../docs && webpack --config webpack.prod.js && npm run copy:static",
    "copy:static": "cp -rf ./src/statics ../docs",
    "dev:parcel": "parcel ./src/index.html --open"
  },
  "author": "amazingandyyy",
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-sass": "2.8.2",
    "buffer": "^5.5.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^8.0.1",
    "node-sass": "^7.0.3",
    "sass-loader": "^6.0.6",
    "serve": "^9.4.0",
    "style-loader": "^0.20.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.11.5",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^0.27.2",
    "parcel": "^2.8.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.0.0",
    "react-router-dom": "^6.0.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.1.0",
    "use-form-react": "0.0.9"
  }
}
